{
    "name": "n8n-nodes-javelin",
    "version": "0.1.1",
    "description": "Javelin node provides AI safety and content moderation services with support for prompt injection detection, trust & safety violations, sensitive data loss protection, language detection and other guardrails",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Abhijit",
        "email": "abhijit.hema@getjavelin.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/abhijitjavelin/n8n-nodes-javelin.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist",
        "icons"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/JavelinGuardrailsApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/JavelinGuardrails/JavelinGuardrails.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
